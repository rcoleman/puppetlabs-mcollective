# Basic stuff
topicprefix     = <%= scope.lookupvar("mcollective::params::mc_topicprefix") %>
main_collective = <%= scope.lookupvar("mcollective::params::main_collective") %>
collectives     = <%= scope.lookupvar("mcollective::params::collectives") %>
libdir          = <%= scope.lookupvar("mcollective::params::mc_libdir") %>
loglevel        = <%= scope.lookupvar("mcollective::params::mc_loglevel") %>

# Plugins
securityprovider = <%= scope.lookupvar("mcollective::params::mc_security_provider") %>
plugin.psk       = <%= scope.lookupvar("mcollective::params::mc_security_psk") %>

# Middleware
connector              = <%= scope.lookupvar("mcollective::params::connector") %>
plugin.stomp.pool.size = <%= scope.lookupvar("mcollective::params::stomp_pool_size") %>
<% stomp_pool.each do |pool,configs| -%>
<% configs.sort.each do |key,value| -%>
plugin.stomp.pool.<%= key -%> = <%= value %>
<% end -%>
<% end -%>
